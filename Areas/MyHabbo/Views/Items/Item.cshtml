@model KeplerCMS.Areas.MyHabbo.Models.ItemViewModel;
@{
    if (Model.Definition.Type == "widgets" && Model.WidgetData != null)
    {
        <partial name="~/Areas/MyHabbo/Views/Items/Widget.cshtml" model=Model />
    }
    else if (Model.Definition.Type == "stickers")
    {
        <partial name="~/Areas/MyHabbo/Views/Items/Sticker.cshtml" model=Model />
    }
    else if (Model.Definition.Type == "notes")
    {
        <partial name="~/Areas/MyHabbo/Views/Items/Stickie.cshtml" model=Model />
    }
    else
    {
        <strong>ITEM TYPE NOT IMPLEMENTED</strong>
    }
}