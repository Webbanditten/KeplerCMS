@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@model KeplerCMS.Areas.MyHabbo.Models.ItemViewModel;
@{Layout = null;}


<div class="widget-corner" id="widget-@(Model.Item.Id)-handle">
    <div class="widget-headline">
        <h3>
            @{
                if (Model.EnableEditing)
                {
                    <img src="~/c_images/myhabbo/icon_edit.gif" width="19" height="18" class="edit-button" id="widget-@Model.Item.Id-edit">
                    <script language="JavaScript" type="text/javascript">
                                        Event.observe("widget-@Model.Item.Id-edit", "click", function (e) { openEditMenu(e, @Model.Item.Id, "widget", "widget-@Model.Item.Id-edit"); }, false);
                    </script>
                }
            }
            <span class="header-left">&nbsp;</span><span class="header-middle">HIGH SCORES</span><span class="header-right">&nbsp;</span>
        </h3>
    </div>
</div>
<div class="widget-body">
    <div class="widget-content">
        <table>
            <tbody>
                <tr colspan="2">
                    <th><a href="https://web.archive.org/web/20070209231255/http://www.habbo.dk/games/battle_ball/">Battle Ball</a></th>
                </tr>
                <tr>
                    <td>Antal spil</td>
                    <td>13</td>
                </tr>
                <tr>
                    <td>Samlet score</td>
                    <td>31225</td>
                </tr>
                <tr colspan="2">
                    <th><a href="https://web.archive.org/web/20070209231255/http://www.habbo.dk/games/wobble_squabble/">Wobble Squabble</a></th>
                </tr>
                <tr>
                    <td>Antal spil</td>
                    <td>2</td>
                </tr>
                <tr>
                    <td>Samlet score</td>
                    <td>4</td>
                </tr>
                <tr colspan="2">
                    <th><a href="https://web.archive.org/web/20070209231255/http://www.habbo.dk/games/snowstorm/">Snow Storm</a></th>
                </tr>
                <tr>
                    <td>Antal spil</td>
                    <td>17</td>
                </tr>
                <tr>
                    <td>Samlet score</td>
                    <td>382</td>
                </tr>
            </tbody>
        </table>
        <div class="clear"></div>
    </div>
</div>